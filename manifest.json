{
    "manifest_version": 3,
    "name": "Chrome Workspaces",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "tabGroups"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "resources/images/hello_extension.png"
    },
    "options_page": "options/options.html"
}